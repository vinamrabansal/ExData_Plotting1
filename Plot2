proj_set<-read.csv("HPC.txt",sep=";")
proj_set2 <- subset(proj_set,(proj_set$Date=="1/2/2007" | proj_set$Date == "2/2/2007"))
proj_set2$DT <- paste(as.Date(as.character(proj_set2$Date),"%d/%m/%Y"),proj_set2$Time,sep=" ")
png(file="Plot2.png")
plot( as.POSIXlt(proj_set2$DT) , as.numeric(as.character((proj_set2[,3]))),type="l" , ylab = 'Global Active Power ( Kilowatts)' , cex.lab=0.8 , xlab='',cex.axis = 0.8)
dev.off()